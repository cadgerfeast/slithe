{
	"extends": "./.sync/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"paths": {
			"$lib":["./src"],
  		"$lib/*":["./src/*"],
			"@/*": ["./src/*"]
		}
	}
}
