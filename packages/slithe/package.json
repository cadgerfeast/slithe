{
  "name": "slithe",
  "version": "0.2.12",
  "description": "Snaky web components that moults",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cadgerfeast/slithe.git"
  },
  "bugs": {
    "url": "https://github.com/cadgerfeast/slithe/issues"
  },
  "keywords": [
    "slithe",
    "snake",
    "moult",
    "svelte",
    "web-components",
    "custom-elements"
  ],
  "author": "Jérémie Pichon <contact@cadgerfeast.com>",
  "homepage": "https://slithe.cadgerfeast.dev",
  "files": [
    "dist",
    "primer",
    "vue"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:lib": "stencil build --watch",
    "dev:themes": "vite build -w -c ./themes/vite.config.js",
    "build": "run-s build:*",
    "build:lib": "stencil build",
    "build:themes": "vite build -c ./themes/vite.config.js",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "test": "stencil test --spec --e2e",
    "generate": "node ./devops/npm/generate/index.js"
  },
  "dependencies": {
    "@stencil/core": "^4.9.0"
  },
  "devDependencies": {
    "@primer/octicons": "^17.11.1",
    "@stencil-community/eslint-plugin": "^0.7.1",
    "@stencil/sass": "^3.0.8",
    "@stencil/store": "^2.0.12",
    "@stencil/vue-output-target": "^0.8.7",
    "@types/eslint": "^8.56.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/primer__octicons": "^17.5.0",
    "ansi-colors": "^4.1.3",
    "eslint": "^8.56.0",
    "eta": "^2.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^19.5.2",
    "sass": "^1.69.5",
    "vite": "^4.1.1"
  },
  "license": "MIT"
}
