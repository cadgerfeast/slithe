{
	"name": "slithe",
	"version": "0.0.2",
	"type": "module",
	"main": "./components/slithe.cjs",
  "module": "./components/slithe.js",
  "unpkg": "./components/slithe.umd.cjs",
	"types": "./types/types.d.ts",
	"exports": {
    ".": {
      "import": "./components/slithe.js",
      "require": "./components/slithe.cjs",
      "types": "./types/types.d.ts"
    }
  },
	"files": [
    "components",
		"themes",
		"types"
  ],
	"keywords": [
		"slithe",
		"snake",
		"moult",
		"svelte",
		"web-components",
		"custom-elements"
	],
	"author": "Jérémie Pichon <contact@cadgerfeast.com>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cadgerfeast/slithe.git"
	},
	"bugs": {
		"url": "https://github.com/cadgerfeast/slithe/issues"
	},
	"license": "MIT",
	"scripts": {
		"dev": "run-p dev:*",
		"dev:lib": "vite build -w",
		"dev:themes": "vite build -w -c vite.themes.config.js",
		"dev:dts": "tsc -w -p tsconfig.dts.json",
		"dev:docs": "madoc dev",
		"build": "run-s build:*",
		"build:lib": "vite build",
		"build:themes": "vite build -c vite.themes.config.js",
		"build:dts": "tsc -p tsconfig.dts.json",
		"build:docs": "madoc build",
		"preview": "madoc preview",
		"lint": "run-s lint:*",
		"lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
		"lint:tsc": "eslint .",
		"test": "vitest run"
	},
	"dependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@madoc/core": "^0.2.3",
		"@primer/octicons": "^17.11.1",
		"@sveltejs/vite-plugin-svelte": "^2.0.0",
		"@types/primer__octicons": "^17.5.0",
		"@tsconfig/svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"sass": "^1.57.1",
		"svelte-check": "^2.10.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	}
}
