{
	"name": "slithe",
	"version": "0.0.2",
	"type": "module",
	"keywords": [
    "slithe",
    "snake",
    "moult",
    "svelte",
    "web-components",
    "custom-elements"
  ],
	"author": "Jérémie Pichon <contact@cadgerfeast.com>",
	"repository": {
    "type": "git",
    "url": "git+https://github.com/cadgerfeast/slithe.git"
  },
	"bugs": {
    "url": "https://github.com/cadgerfeast/slithe/issues"
  },
	"license": "MIT",
	"scripts": {
		"sync": "svelte-kit sync",
		"dev": "run-s sync && run-p dev:*",
		"dev:lib": "svelte-package -w",
		"dev:themes": "vite build -w -c vite.themes.config.js",
		"dev:docs": "madoc dev",
		"build": "run-s sync build:*",
		"build:lib": "svelte-package",
		"build:themes": "vite build -c vite.themes.config.js",
		"build:docs": "madoc build",
		"lint": "run-s sync lint:*",
		"lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
		"lint:tsc": "eslint .",
		"test": "vitest run"
	},
	"dependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@madoc/core": "^0.2.2",
		"@sveltejs/kit": "^1.0.0-next.587",
		"@sveltejs/package": "^1.0.0-next.6",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"sass": "^1.57.1",
		"svelte-check": "^2.9.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	}
}
