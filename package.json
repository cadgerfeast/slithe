{
  "name": "slithe",
  "version": "0.0.1",
  "type": "module",
  "main": "./components/index.cjs.js",
  "module": "./components/index.es.js",
  "unpkg": "./components/index.umd.js",
  "keywords": [
    "slithe",
    "snake",
    "moult",
    "svelte",
    "web-components",
    "custom-elements"
  ],
  "author": "Jérémie Pichon <contact@cadgerfeast.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cadgerfeast/slithe.git"
  },
  "bugs": {
    "url": "https://github.com/cadgerfeast/slithe/issues"
  },
  "license": "MIT",
  "files": [
    "components",
    "themes"
  ],
  "scripts": {
    "dev": "run-s build:lib build:themes && run-p dev:lib dev:themes dev:docs",
    "dev:lib": "run-s dev:lib:bundle dev:lib:splits",
    "dev:lib:bundle": "vite build --watch -c vite.lib.bundle.config.ts",
    "dev:lib:splits": "vite build --watch -c vite.lib.splits.config.ts",
    "dev:themes": "vite build --watch -c vite.themes.config.ts",
    "dev:docs": "vite",
    "build": "run-s build:lib build:themes build:docs",
    "build:lib": "run-s build:lib:bundle build:lib:splits",
    "build:lib:bundle": "vite build -c vite.lib.bundle.config.ts",
    "build:lib:splits": "vite build -c vite.lib.splits.config.ts",
    "build:themes": "vite build -c vite.themes.config.ts",
    "build:docs": "vite build",
    "lint": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "serve": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.41",
    "@tsconfig/svelte": "2.0.1",
    "@types/fs-extra": "9.0.13",
    "c8": "7.11.3",
    "fs-extra": "10.1.0",
    "jsdom": "19.0.0",
    "mdsvex": "0.10.5",
    "npm-run-all": "4.1.5",
    "sass": "1.50.0",
    "svelte": "3.47.0",
    "svelte-check": "2.7.1",
    "svelte-navigator": "3.1.5",
    "svelte-preprocess": "4.10.6",
    "tslib": "2.3.1",
    "typescript": "4.6.3",
    "vite": "2.9.5",
    "vitest": "0.12.9"
  }
}
