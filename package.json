{
  "name": "slithe",
  "version": "0.2.6",
  "description": "Snaky web components that moults",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/slithe/slithe.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cadgerfeast/slithe.git"
  },
  "bugs": {
    "url": "https://github.com/cadgerfeast/slithe/issues"
  },
  "keywords": [
    "slithe",
    "snake",
    "moult",
    "svelte",
    "web-components",
    "custom-elements"
  ],
  "author": "Jérémie Pichon <contact@cadgerfeast.com>",
  "files": [
    "dist",
    "primer"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:lib": "stencil build --watch",
    "dev:themes": "vite build -w -c ./themes/vite.config.js",
    "dev:docs": "madoc dev",
    "build": "run-s build:*",
    "build:lib": "stencil build",
    "build:themes": "vite build -c ./themes/vite.config.js",
    "build:docs": "madoc build",
    "preview": "madoc preview",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "test": "stencil test --spec --e2e",
    "generate": "node ./devops/npm/generate/index.js"
  },
  "dependencies": {
    "@stencil/core": "^3.0.0"
  },
  "devDependencies": {
    "@madoc/core": "^0.2.4",
    "@primer/octicons": "^17.11.1",
    "@stencil-community/eslint-plugin": "^0.5.0",
    "@stencil/sass": "^2.0.3",
    "@stencil/store": "^2.0.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/primer__octicons": "^17.5.0",
    "ansi-colors": "^4.1.3",
    "eta": "^2.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^19.5.2",
    "vite": "^4.1.1"
  },
  "license": "MIT"
}
